{
  "name": "traily-nova-extension",
  "version": "1.0.0",
  "description": "Removes invisible characters and ensures an empty line at each file's end.",
  "private": true,
  "license": "MIT",
  "keywords": [
    "Nova.app",
    "Nova Extension",
    "RapydScript"
  ],
  "author": {
    "name": "Denny Trebbin"
  },
  "scripts": {
    "build:pretty": "rapydscript src/main.pyj --output build/main.pyj.js --es6 --bare --prettify",
    "build": "rapydscript src/main.pyj --output build/main.pyj.js --es6 --bare --dd --di"
  },
  "type": "commonjs",
  "devDependencies": {
    "npm": "^7.4.3",
    "rapydscript": "^0.5.62"
  }
}
